{
  "version": 2,
  "projects": {
    "ci-config": {
      "root": "libs/ci-config",
      "sourceRoot": "libs/ci-config/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/js:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/ci-config",
            "main": "libs/ci-config/src/index.ts",
            "tsConfig": "libs/ci-config/tsconfig.lib.json",
            "assets": ["libs/ci-config/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/ci-config/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ci-config"],
          "options": {
            "jestConfig": "libs/ci-config/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "cy-scaffold": {
      "root": "libs/cy-scaffold",
      "sourceRoot": "libs/cy-scaffold/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/cy-scaffold/**/*.{ts,js}"]
          }
        },
        "test": {
          "executor": "@nxext/vitest:vitest",
          "options": {
            "command": "run"
          }
        }
      }
    }
  }
}
